<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.api.flobankapi.mapper.ExtAcctMapper" >
    <select id="selectExtAcct">
        SELECT * FROM TB_EXT_ACCT
        WHERE EXT_NO = #{acctNo} AND EXT_BK_CODE = #{bkCode}
    </select>
</mapper>