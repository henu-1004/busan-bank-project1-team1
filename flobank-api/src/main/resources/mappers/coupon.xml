<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.api.flobankapi.mapper.CouponMapper">
    <update id="updateAcctBal">
        UPDATE TB_COUPON
        SET COUP_STATUS = 0
        WHERE COUP_NO = #{couponNo}
    </update>
</mapper>
